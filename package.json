{
  "name": "elections",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "license": "MIT",
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "format": "prettier --write \"src/**/*.ts\"",
    "start": "ts-node -r tsconfig-paths/register src/main.ts",
    "start:dev": "nodemon",
    "start:debug": "nodemon --config nodemon-debug.json",
    "prestart:prod": "rimraf dist && npm run build",
    "start:prod": "node dist/main.js",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "seed": "ts-node ./node_modules/typeorm-seeding/dist/cli.js seed  --config src/ormconfig.ts ",
    "typeorm": "cross-env NODE_ENV=development ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js --config src/ormconfig.ts",
    "typeorm:migrate": "npm run typeorm migration:generate -- -n",
    "typeorm:run": "npm run typeorm migration:run",
    "typeorm:revert": "npm run typeorm migration:revert",
    "typeorm:prod": "cross-env NODE_ENV=production ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js --config src/ormconfig.ts --connection colegiodb",
    "typeorm:migrate:prod": "npm run typeorm:prod migration:generate -- -n",
    "typeorm:run:prod": "npm run typeorm:prod migration:run",
    "typeorm:test": "cross-env NODE_ENV=testing ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js --config src/ormconfig.ts --connection colegiodb",
    "typeorm:migrate:test": "npm run typeorm:test migration:generate -- -n",
    "typeorm:run:test": "npm run typeorm:test migration:run"
  },
  "dependencies": {
    "@hapi/hapi": "^18.4.0",
    "@nestjs/common": "^6.8.2",
    "@nestjs/core": "^6.8.2",
    "@nestjs/graphql": "^6.5.3",
    "@nestjs/jwt": "^6.1.1",
    "@nestjs/passport": "^6.1.0",
    "@nestjs/platform-express": "^6.8.2",
    "@nestjs/typeorm": "^6.2.0",
    "@nestjsx/crud": "^4.2.0",
    "@nestjsx/crud-request": "^4.2.0",
    "@nestjsx/crud-typeorm": "^4.2.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/faker": "^4.1.6",
    "apollo-server-express": "^2.9.5",
    "bcryptjs": "^2.4.3",
    "class-transformer": "^0.2.3",
    "faker": "^4.1.0",
    "graphql": "^14.5.8",
    "graphql-tools": "^4.0.5",
    "mysql": "^2.17.1",
    "passport": "^0.4.0",
    "passport-http-bearer": "^1.0.1",
    "passport-jwt": "^4.0.0",
    "reflect-metadata": "^0.1.12",
    "rimraf": "^2.7.1",
    "rxjs": "^6.5.3",
    "typeorm": "^0.2.19",
    "typeorm-seeding": "^1.0.0-rc.3"
  },
  "devDependencies": {
    "@nestjs/testing": "^6.8.2",
    "@types/express": "^4.17.1",
    "@types/jest": "^23.3.13",
    "@types/node": "^10.14.20",
    "@types/supertest": "^2.0.8",
    "jest": "^23.6.0",
    "nodemon": "^1.19.3",
    "prettier": "^1.18.2",
    "supertest": "^3.4.1",
    "ts-jest": "^23.10.5",
    "ts-node": "^7.0.1",
    "tsconfig-paths": "^3.9.0",
    "tslint": "5.12.1",
    "typescript": "^3.6.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
