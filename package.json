{
  "name": "elections",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "license": "MIT",
  "scripts": {
    "build": "rimraf dist && tsc -p tsconfig.build.json",
    "format": "prettier --write \"src/**/*.ts\"",
    "start": "cross-env NODE_ENV=development ts-node -r tsconfig-paths/register src/main.ts",
    "start:dev": "cross-env NODE_ENV=development tsc-watch -p tsconfig.build.json --onSuccess \"node dist/main.js\"",
    "start:debug": "cross-env NODE_ENV=development tsc-watch -p tsconfig.build.json --onSuccess \"node --inspect-brk dist/main.js\"",
    "start:prod": "cross-env NODE_ENV=production node dist/main.js",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "seed": "ts-node ./node_modules/typeorm-seeding/dist/cli.js seed  --config src/ormconfig.ts ",
    "typeorm": "cross-env NODE_ENV=development ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js --config src/ormconfig.ts",
    "typeorm:migrate": "npm run typeorm migration:generate -- -n",
    "typeorm:run": "npm run typeorm migration:run",
    "typeorm:revert": "npm run typeorm migration:revert",
    "typeorm:prod": "cross-env NODE_ENV=production ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js --config src/ormconfig.ts --connection colegiodb",
    "typeorm:migrate:prod": "npm run typeorm:prod migration:generate -- -n",
    "typeorm:run:prod": "npm run typeorm:prod migration:run",
    "typeorm:test": "cross-env NODE_ENV=testing ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js --config src/ormconfig.ts --connection colegiodb",
    "typeorm:migrate:test": "npm run typeorm:test migration:generate -- -n",
    "typeorm:run:test": "npm run typeorm:test migration:run"
  },
  "engines": {
    "node": "14.15.1"
  },
  "dependencies": {
    "@hapi/hapi": "^18.4.1",
    "@nestjs/common": "^6.11.11",
    "@nestjs/core": "^6.11.11",
    "@nestjs/graphql": "^6.6.2",
    "@nestjs/jwt": "^6.1.2",
    "@nestjs/passport": "^6.2.0",
    "@nestjs/platform-express": "^6.11.11",
    "@nestjs/swagger": "^4.7.11",
    "@nestjs/typeorm": "^6.3.4",
    "@nestjsx/crud": "^4.6.2",
    "@nestjsx/crud-request": "^4.6.2",
    "@nestjsx/crud-typeorm": "^4.6.2",
    "@types/bcryptjs": "^2.4.2",
    "@types/faker": "^4.1.12",
    "apollo-server-express": "^2.19.2",
    "bcrypt": "^5.0.0",
    "bcryptjs": "^2.4.3",
    "class-transformer": "^0.2.3",
    "class-validator": "^0.13.1",
    "express-session": "^1.17.1",
    "faker": "^4.1.0",
    "graphql": "^14.7.0",
    "graphql-tools": "^4.0.8",
    "moment": "^2.29.1",
    "mysql": "^2.18.1",
    "passport": "^0.4.1",
    "passport-http-bearer": "^1.0.1",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "reflect-metadata": "^0.1.12",
    "rimraf": "^2.7.1",
    "rxjs": "^6.6.3",
    "sqlite3": "^5.0.1",
    "swagger-ui-express": "^4.1.6",
    "typeorm": "^0.2.30",
    "typeorm-seeding": "^1.6.1"
  },
  "devDependencies": {
    "@nestjs/testing": "^6.11.11",
    "@types/express": "^4.17.11",
    "@types/jest": "^23.3.13",
    "@types/node": "^10.17.51",
    "@types/passport-local": "^1.0.33",
    "@types/supertest": "^2.0.10",
    "jest": "^23.6.0",
    "nodemon": "^1.19.4",
    "prettier": "^1.19.1",
    "supertest": "^3.4.1",
    "ts-jest": "^23.10.5",
    "ts-node": "^7.0.1",
    "tsconfig-paths": "^3.9.0",
    "tslint": "5.12.1",
    "typescript": "^3.9.7"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
